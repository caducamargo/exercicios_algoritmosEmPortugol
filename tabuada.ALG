Algoritmo "semnome"

Var
   nt, divi, cont: inteiro


Inicio
      escreva ("De qual número você quer consultar a tabuada? ")
      leia (nt)
      cont <- 1
      enquanto (cont <= 10) faca
               divi <- nt * cont
               escreval (nt, " X", cont, " =", divi)
               cont <- cont + 1

      FimEnquanto




Fimalgoritmo