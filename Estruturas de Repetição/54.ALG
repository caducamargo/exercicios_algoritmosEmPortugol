Algoritmo "ContadorInteligente2"

// Autor(a)    : Cadu

Var
   n, contador: inteiro



Inicio
      repita
      escreva ("Até qual número você quer contar? [ACIMA DE 0]: ")
      leia (n)
      até (n > 0)
      para contador de 1 até n faca
           escreval (contador)
           contador <- contador +1
      FimPara


Fimalgoritmo