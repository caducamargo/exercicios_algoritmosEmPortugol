Algoritmo "Soma10Valores"

// Autor(a)    : Cadu

Var
n, c, S: inteiro


Inicio
      c <- 1
      S <- 0
      para n de 1 até 10 faca
           escreva ("Digíte o",c, "º número: " )
           leia (n)
           S <- S + n
           c <- c + 1
      FimPara
      escreval ("")
      escreval ("A soma dos valores digítados é:", S )


Fimalgoritmo