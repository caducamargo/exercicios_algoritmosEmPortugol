Algoritmo "ContadorInteligente"

// Autor(a)    : Cadu

Var
   n, contador: inteiro



Inicio
      escreva ("Até qual número você quer contar? [ACIMA DE 0]: ")
      leia (n)
      para contador de 1 até n faca
           escreval (contador)
           contador <- contador +1
      FimPara


Fimalgoritmo