Algoritmo "ImparPar"

Var
   numero: inteiro


Inicio
      escreva("Escreva um número qualquer: ")
       leia(numero)

       se (numero mod 2 = 0) entao
          escreva("O número ", numero, " é par!")
       senao
           escreva("O número ", numero, " é ímpar!")
       fimse

      

Fimalgoritmo